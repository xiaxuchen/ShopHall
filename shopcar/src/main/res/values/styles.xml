<resources xmlns:tools="http://schemas.android.com/tools">

    <!-- Base application theme. -->
    <style name="AppTheme" parent="QMUI.Compat.NoActionBar" tools:ignore="ResourceName">
        <!-- Customize your theme here. -->
        <item name="colorPrimary">@color/colorPrimary</item>
        <item name="colorPrimaryDark">@color/colorPrimaryDark</item>
        <item name="colorAccent">@color/colorAccent</item>
    </style>
    <style name="AppThem2" parent="QMUI.Compat.NoActionBar">
        <item name="android:windowBackground">#ffffffff</item>
        <item name="android:windowIsTranslucent">true</item>
        <item name="android:windowContentOverlay">@null</item>
    </style>
    <style name="AppThem3" parent="QMUI.Compat.NoActionBar">
        <item name="android:windowBackground">@drawable/pbg14</item>
        <item name="android:windowIsTranslucent">false</item>
        <item name="android:windowFullscreen">true</item>
    </style>
</resources>
