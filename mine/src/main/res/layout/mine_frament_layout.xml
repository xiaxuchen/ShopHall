<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#f5f5f9"
    tools:context=".fragment.MineFragment">
    <androidx.constraintlayout.widget.ConstraintLayout
        android:id="@+id/mineHeadBar"
        android:layout_width="match_parent"
        android:layout_height="100dp"
        android:background="#fd4369"
        tools:ignore="MissingConstraints">
        <com.qmuiteam.qmui.widget.QMUIRadiusImageView
            android:id="@+id/qmuiIvAvatar"
            android:layout_width="70dp"
            android:layout_height="70dp"
            android:background="@color/common_blue"
            android:layout_marginLeft="@dimen/common_margin"
            android:layout_marginTop="20dp"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintLeft_toLeftOf="parent"
            />
        <TextView
            android:id="@+id/tvUsername"
            android:layout_width="wrap_content"
            android:layout_height="23dp"
            android:text="淘淘猫"
            android:textColor="@color/white"
            android:textSize="14dp"
            android:padding="3dp"
            android:layout_marginTop="30dp"
            android:layout_marginLeft="20dp"
            app:layout_constraintLeft_toRightOf="@id/qmuiIvAvatar"
            app:layout_constraintTop_toTopOf="parent"
            />
        <TextView
            android:id="@+id/tvSetUpInfo"
            android:layout_width="wrap_content"
            android:layout_height="20dp"
            android:text="编辑资料>"
            android:textColor="@color/white"
            android:layout_marginTop="10dp"
            android:layout_marginLeft="20dp"
            app:layout_constraintLeft_toRightOf="@id/qmuiIvAvatar"
            app:layout_constraintTop_toBottomOf="@id/tvUsername"/>
        <ImageView
            android:id="@+id/imMineSetUp"
            android:layout_width="20dp"
            android:layout_height="20dp"
            android:src="@mipmap/mine_set_up"
            android:layout_marginTop="10dp"
            android:layout_marginRight="15dp"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintRight_toRightOf="parent"
            />
    </androidx.constraintlayout.widget.ConstraintLayout>
    <LinearLayout
        android:id="@+id/clData"
        android:layout_width="match_parent"
        android:layout_height="70dp"
        android:background="@color/white"
        android:orientation="horizontal"
        app:layout_constraintTop_toBottomOf="@id/mineHeadBar"
        tools:ignore="MissingConstraints">
        <androidx.constraintlayout.widget.ConstraintLayout
            android:layout_width="0dp"
            android:layout_height="match_parent"
            android:layout_weight="1">
            <TextView
                android:id="@+id/tvFunds"
                android:layout_width="wrap_content"
                android:layout_height="30dp"
                android:text="￥0.00"
                android:textSize="20dp"
                android:textColor="#e53b8e"
                android:layout_marginTop="10dp"
                app:layout_constraintTop_toTopOf="parent"
                app:layout_constraintLeft_toLeftOf="parent"
                app:layout_constraintRight_toRightOf="parent"/>
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="20dp"
                android:text="资金"
                android:textColor="#7c7c7c"
                app:layout_constraintLeft_toLeftOf="parent"
                app:layout_constraintRight_toRightOf="parent"
                app:layout_constraintTop_toBottomOf="@id/tvFunds"/>

            <View
                android:layout_width="1dp"
                android:layout_height="40dp"
                android:layout_marginTop="15dp"
                android:background="#dfdfdf"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintRight_toRightOf="parent"
                app:layout_constraintTop_toTopOf="parent" />
        </androidx.constraintlayout.widget.ConstraintLayout>
        <androidx.constraintlayout.widget.ConstraintLayout
            android:layout_width="0dp"
            android:layout_height="match_parent"
            android:layout_weight="1">
            <TextView
                android:id="@+id/tvIntegral"
                android:layout_width="wrap_content"
                android:layout_height="30dp"
                android:text="00"
                android:textSize="20dp"
                android:textColor="#e53b8e"
                android:layout_marginTop="10dp"
                app:layout_constraintTop_toTopOf="parent"
                app:layout_constraintLeft_toLeftOf="parent"
                app:layout_constraintRight_toRightOf="parent"/>
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="20dp"
                android:text="积分"
                android:textColor="#7c7c7c"
                app:layout_constraintLeft_toLeftOf="parent"
                app:layout_constraintRight_toRightOf="parent"
                app:layout_constraintTop_toBottomOf="@id/tvIntegral"/>

            <View
                android:layout_width="1dp"
                android:layout_height="40dp"
                android:layout_marginTop="15dp"
                android:background="#dfdfdf"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintRight_toRightOf="parent"
                app:layout_constraintTop_toTopOf="parent" />
        </androidx.constraintlayout.widget.ConstraintLayout>
        <androidx.constraintlayout.widget.ConstraintLayout
            android:layout_width="0dp"
            android:layout_height="match_parent"
            android:layout_weight="1">
            <TextView
                android:id="@+id/tvEffectiveness"
                android:layout_width="wrap_content"
                android:layout_height="30dp"
                android:text="0%"
                android:textSize="20dp"
                android:textColor="#e53b8e"
                android:layout_marginTop="10dp"
                app:layout_constraintTop_toTopOf="parent"
                app:layout_constraintLeft_toLeftOf="parent"
                app:layout_constraintRight_toRightOf="parent"/>
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="20dp"
                android:text="下单效率"
                android:textColor="#7c7c7c"
                app:layout_constraintLeft_toLeftOf="parent"
                app:layout_constraintRight_toRightOf="parent"
                app:layout_constraintTop_toBottomOf="@id/tvEffectiveness"/>
        </androidx.constraintlayout.widget.ConstraintLayout>
    </LinearLayout>
    <View
        android:id="@+id/divider01"
        android:layout_width="match_parent"
        android:layout_height="10dp"
        tools:ignore="MissingConstraints"
        app:layout_constraintTop_toBottomOf="@id/clData"/>
    <TextView
        android:id="@+id/tvTask"
        android:layout_width="match_parent"
        android:layout_height="30dp"
        android:text="当日任务"
        android:textColor="#e53b8e"
        android:background="@color/white"
        android:gravity="center_vertical"
        android:paddingLeft="20dp"
        app:layout_constraintTop_toBottomOf="@id/divider01"
        tools:ignore="MissingConstraints" />
    <LinearLayout
        android:id="@+id/Task"
        android:layout_width="match_parent"
        android:layout_height="70dp"
        android:background="@color/white"
        tools:ignore="MissingConstraints"
        app:layout_constraintTop_toBottomOf="@id/tvTask">
        <androidx.constraintlayout.widget.ConstraintLayout
            android:layout_width="0dp"
            android:layout_height="match_parent"
            android:layout_weight="1"
            >
            <View
                android:layout_width="match_parent"
                android:layout_height="1dp"
                android:background="#e53b8e"
                app:layout_constraintTop_toTopOf="parent"/>
            <ImageView
                android:id="@+id/imScreenshot"
                android:layout_width="30dp"
                android:layout_height="30dp"
                android:src="@mipmap/mine_upload"
                android:layout_marginTop="10dp"
                app:layout_constraintTop_toTopOf="parent"
                app:layout_constraintLeft_toLeftOf="parent"
                app:layout_constraintRight_toRightOf="parent"/>
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="待上传截图"
                android:textColor="#7c7c7c"
                android:textSize="10dp"
                android:layout_marginTop="5dp"
                app:layout_constraintTop_toBottomOf="@id/imScreenshot"
                app:layout_constraintLeft_toLeftOf="parent"
                app:layout_constraintRight_toRightOf="parent"/>
        </androidx.constraintlayout.widget.ConstraintLayout>
        <androidx.constraintlayout.widget.ConstraintLayout
            android:layout_width="0dp"
            android:layout_height="match_parent"
            android:layout_weight="1"
            >
            <View
                android:layout_width="match_parent"
                android:layout_height="1dp"
                android:background="#e53b8e"
                app:layout_constraintTop_toTopOf="parent"/>
            <ImageView
                android:id="@+id/imFillIn"
                android:layout_width="30dp"
                android:layout_height="30dp"
                android:src="@mipmap/mine_application"
                android:layout_marginTop="10dp"
                app:layout_constraintTop_toTopOf="parent"
                app:layout_constraintLeft_toLeftOf="parent"
                app:layout_constraintRight_toRightOf="parent"/>
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="待填写订单"
                android:textColor="#7c7c7c"
                android:textSize="10dp"
                android:layout_marginTop="5dp"
                app:layout_constraintTop_toBottomOf="@id/imFillIn"
                app:layout_constraintLeft_toLeftOf="parent"
                app:layout_constraintRight_toRightOf="parent"/>
        </androidx.constraintlayout.widget.ConstraintLayout>
        <androidx.constraintlayout.widget.ConstraintLayout
            android:layout_width="0dp"
            android:layout_height="match_parent"
            android:layout_weight="1"
            >
            <View
                android:layout_width="match_parent"
                android:layout_height="1dp"
                android:background="#e53b8e"
                app:layout_constraintTop_toTopOf="parent"/>
            <ImageView
                android:id="@+id/imreport"
                android:layout_width="30dp"
                android:layout_height="30dp"
                android:src="@mipmap/mine_report"
                android:layout_marginTop="10dp"
                app:layout_constraintTop_toTopOf="parent"
                app:layout_constraintLeft_toLeftOf="parent"
                app:layout_constraintRight_toRightOf="parent"/>
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="待填写报告"
                android:textColor="#7c7c7c"
                android:textSize="10dp"
                android:layout_marginTop="5dp"
                app:layout_constraintTop_toBottomOf="@id/imreport"
                app:layout_constraintLeft_toLeftOf="parent"
                app:layout_constraintRight_toRightOf="parent"/>
        </androidx.constraintlayout.widget.ConstraintLayout>
    </LinearLayout>
    <View
        android:id="@+id/divider02"
        android:layout_width="match_parent"
        android:layout_height="10dp"
        app:layout_constraintTop_toBottomOf="@id/Task"
        android:background="#f5f5f9"
        tools:ignore="MissingConstraints" />
    <TextView
        android:id="@+id/tvTryUser"
        android:layout_width="match_parent"
        android:layout_height="30dp"
        android:text="普通使用"
        android:textColor="#e53b8e"
        android:background="@color/white"
        android:gravity="center_vertical"
        android:paddingLeft="20dp"
        app:layout_constraintTop_toBottomOf="@id/divider02"
        tools:ignore="MissingConstraints" />
    <LinearLayout
        android:id="@+id/TryUser"
        android:layout_width="match_parent"
        android:layout_height="70dp"
        android:background="@color/white"
        tools:ignore="MissingConstraints"
        app:layout_constraintTop_toBottomOf="@id/tvTryUser">
        <androidx.constraintlayout.widget.ConstraintLayout
            android:layout_width="0dp"
            android:layout_height="match_parent"
            android:layout_weight="1"
            >
            <View
                android:layout_width="match_parent"
                android:layout_height="1dp"
                android:background="#e53b8e"
                app:layout_constraintTop_toTopOf="parent"/>
            <ImageView
                android:id="@+id/imApplication"
                android:layout_width="30dp"
                android:layout_height="30dp"
                android:src="@mipmap/mine_application"
                android:layout_marginTop="10dp"
                app:layout_constraintTop_toTopOf="parent"
                app:layout_constraintLeft_toLeftOf="parent"
                app:layout_constraintRight_toRightOf="parent"/>
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="待上传截图"
                android:textColor="#7c7c7c"
                android:textSize="10dp"
                android:layout_marginTop="5dp"
                app:layout_constraintTop_toBottomOf="@id/imApplication"
                app:layout_constraintLeft_toLeftOf="parent"
                app:layout_constraintRight_toRightOf="parent"/>
        </androidx.constraintlayout.widget.ConstraintLayout>
        <androidx.constraintlayout.widget.ConstraintLayout
            android:layout_width="0dp"
            android:layout_height="match_parent"
            android:layout_weight="1"
            >
            <View
                android:layout_width="match_parent"
                android:layout_height="1dp"
                android:background="#e53b8e"
                app:layout_constraintTop_toTopOf="parent"/>
            <ImageView
                android:id="@+id/imReview"
                android:layout_width="30dp"
                android:layout_height="30dp"
                android:src="@mipmap/mine_review"
                android:layout_marginTop="10dp"
                app:layout_constraintTop_toTopOf="parent"
                app:layout_constraintLeft_toLeftOf="parent"
                app:layout_constraintRight_toRightOf="parent"/>
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="待审核"
                android:textColor="#7c7c7c"
                android:textSize="10dp"
                android:layout_marginTop="5dp"
                app:layout_constraintTop_toBottomOf="@id/imReview"
                app:layout_constraintLeft_toLeftOf="parent"
                app:layout_constraintRight_toRightOf="parent"/>
        </androidx.constraintlayout.widget.ConstraintLayout>
        <androidx.constraintlayout.widget.ConstraintLayout
            android:layout_width="0dp"
            android:layout_height="match_parent"
            android:layout_weight="1"
            >
            <View
                android:layout_width="match_parent"
                android:layout_height="1dp"
                android:background="#e53b8e"
                app:layout_constraintTop_toTopOf="parent"/>
            <ImageView
                android:id="@+id/imFinished"
                android:layout_width="30dp"
                android:layout_height="30dp"
                android:src="@mipmap/mine_finished"
                android:layout_marginTop="10dp"
                app:layout_constraintTop_toTopOf="parent"
                app:layout_constraintLeft_toLeftOf="parent"
                app:layout_constraintRight_toRightOf="parent"/>
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="已完成试用"
                android:textColor="#7c7c7c"
                android:textSize="10dp"
                android:layout_marginTop="5dp"
                app:layout_constraintTop_toBottomOf="@id/imFinished"
                app:layout_constraintLeft_toLeftOf="parent"
                app:layout_constraintRight_toRightOf="parent"/>
        </androidx.constraintlayout.widget.ConstraintLayout>
        <androidx.constraintlayout.widget.ConstraintLayout
            android:layout_width="0dp"
            android:layout_height="match_parent"
            android:layout_weight="1"
            >
            <View
                android:layout_width="match_parent"
                android:layout_height="1dp"
                android:background="#e53b8e"
                app:layout_constraintTop_toTopOf="parent"/>
            <ImageView
                android:id="@+id/imPraise"
                android:layout_width="30dp"
                android:layout_height="30dp"
                android:src="@mipmap/mine_praise"
                android:layout_marginTop="10dp"
                app:layout_constraintTop_toTopOf="parent"
                app:layout_constraintLeft_toLeftOf="parent"
                app:layout_constraintRight_toRightOf="parent"/>
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="需要追评"
                android:textColor="#7c7c7c"
                android:textSize="10dp"
                android:layout_marginTop="5dp"
                app:layout_constraintTop_toBottomOf="@id/imPraise"
                app:layout_constraintLeft_toLeftOf="parent"
                app:layout_constraintRight_toRightOf="parent"/>
        </androidx.constraintlayout.widget.ConstraintLayout>
    </LinearLayout>
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:background="@color/white"
            android:layout_marginTop="10dp"
            app:layout_constraintTop_toBottomOf="@id/TryUser"
            android:orientation="vertical">
            <androidx.constraintlayout.widget.ConstraintLayout
                android:layout_width="match_parent"
                android:layout_height="48dp"
                android:layout_marginLeft="20dp"
                >
                <TextView
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:text="一键申请"
                    android:gravity="center_vertical"
                    />
                <ImageView
                    android:layout_width="15dp"
                    android:layout_height="15dp"
                    android:src="@mipmap/mine_back"
                    android:layout_marginTop="16dp"
                    android:layout_marginRight="15dp"
                    app:layout_constraintTop_toTopOf="parent"
                    app:layout_constraintRight_toRightOf="parent"/>
                <View
                    android:layout_width="match_parent"
                    android:layout_height="0.5dp"
                    android:background="#c8c8c8"
                    app:layout_constraintBottom_toBottomOf="parent"/>
            </androidx.constraintlayout.widget.ConstraintLayout>
            <androidx.constraintlayout.widget.ConstraintLayout
                android:layout_width="match_parent"
                android:layout_height="48dp"
                android:layout_marginLeft="20dp"
                >
                <TextView
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:text="资金提现"
                    android:gravity="center_vertical"
                    />
                <ImageView
                    android:layout_width="15dp"
                    android:layout_height="15dp"
                    android:src="@mipmap/mine_back"
                    android:layout_marginTop="16dp"
                    android:layout_marginRight="15dp"
                    app:layout_constraintTop_toTopOf="parent"
                    app:layout_constraintRight_toRightOf="parent"/>
                <View
                    android:layout_width="match_parent"
                    android:layout_height="0.5dp"
                    android:background="#c8c8c8"
                    app:layout_constraintBottom_toBottomOf="parent"/>
            </androidx.constraintlayout.widget.ConstraintLayout>
            <androidx.constraintlayout.widget.ConstraintLayout
                android:layout_width="match_parent"
                android:layout_height="48dp"
                android:layout_marginLeft="20dp"
                >

                <TextView
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:gravity="center_vertical"
                    android:text="资金日志"
                    app:layout_constraintStart_toStartOf="@+id/mineView" />
                <ImageView
                    android:layout_width="15dp"
                    android:layout_height="15dp"
                    android:src="@mipmap/mine_back"
                    android:layout_marginTop="16dp"
                    android:layout_marginRight="15dp"
                    app:layout_constraintTop_toTopOf="parent"
                    app:layout_constraintRight_toRightOf="parent"/>

                <View
                    android:id="@+id/mineView"
                    android:layout_width="match_parent"
                    android:layout_height="0.5dp"
                    android:background="#c8c8c8"
                    app:layout_constraintBottom_toBottomOf="parent" />
            </androidx.constraintlayout.widget.ConstraintLayout>
            <androidx.constraintlayout.widget.ConstraintLayout
                android:layout_width="match_parent"
                android:layout_height="48dp"
                android:layout_marginLeft="20dp"
                >
                <TextView
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:text="账号管理"
                    android:gravity="center_vertical"
                    />
                <ImageView
                    android:layout_width="15dp"
                    android:layout_height="15dp"
                    android:src="@mipmap/mine_back"
                    android:layout_marginTop="16dp"
                    android:layout_marginRight="15dp"
                    app:layout_constraintTop_toTopOf="parent"
                    app:layout_constraintRight_toRightOf="parent"/>
            </androidx.constraintlayout.widget.ConstraintLayout>
            <View
                android:layout_width="match_parent"
                android:background="#f5f5f9"
                android:layout_height="10dp"/>
            <androidx.constraintlayout.widget.ConstraintLayout
                android:layout_width="match_parent"
                android:layout_height="48dp"
                android:layout_marginLeft="20dp"
                >
                <TextView
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:text="联系客服"
                    android:gravity="center_vertical"
                    />
                <ImageView
                    android:layout_width="15dp"
                    android:layout_height="15dp"
                    android:src="@mipmap/mine_back"
                    android:layout_marginTop="16dp"
                    android:layout_marginRight="15dp"
                    app:layout_constraintTop_toTopOf="parent"
                    app:layout_constraintRight_toRightOf="parent"/>
                <View
                    android:layout_width="match_parent"
                    android:layout_height="0.5dp"
                    android:background="#c8c8c8"
                    app:layout_constraintBottom_toBottomOf="parent"/>
            </androidx.constraintlayout.widget.ConstraintLayout>
            <androidx.constraintlayout.widget.ConstraintLayout
            android:layout_width="match_parent"
            android:layout_height="48dp"
            android:layout_marginLeft="20dp"
            >
            <TextView
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:text="用户帮助"
                android:gravity="center_vertical"
                />
            <ImageView
                android:layout_width="15dp"
                android:layout_height="15dp"
                android:src="@mipmap/mine_back"
                android:layout_marginTop="16dp"
                android:layout_marginRight="15dp"
                app:layout_constraintTop_toTopOf="parent"
                app:layout_constraintRight_toRightOf="parent"/>
        </androidx.constraintlayout.widget.ConstraintLayout>
        </LinearLayout>
</androidx.constraintlayout.widget.ConstraintLayout>