<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:orientation="vertical"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/common_bg">

    <androidx.constraintlayout.widget.ConstraintLayout
        android:id="@+id/addressPartOne"
        android:layout_width="match_parent"
        android:layout_height="45dp"
        app:layout_constraintTop_toTopOf="parent"
        tools:ignore="MissingConstraints">
        <View
            android:id="@+id/addressDivider"
            android:layout_width="match_parent"
            android:layout_height="@dimen/common_margin_middle"
            app:layout_constraintTop_toTopOf="parent"
            android:background="@color/common_line_half_a"
            tools:ignore="MissingConstraints" />
        <TextView
            android:id="@+id/addressName"
            android:layout_width="wrap_content"
            android:layout_height="35dp"
            app:layout_constraintLeft_toLeftOf="parent"
            app:layout_constraintTop_toBottomOf="@id/addressDivider"
            android:paddingLeft="@dimen/common_padding"
            android:paddingRight="@dimen/common_padding"
            android:gravity="center"
            android:text=""
            android:textSize="@dimen/text_middle_size"
            android:textColor="@color/black"
            tools:ignore="MissingConstraints" />
        <TextView
            android:id="@+id/addressPhone"
            android:layout_width="wrap_content"
            android:layout_height="35dp"
            app:layout_constraintLeft_toRightOf="@id/addressName"
            app:layout_constraintTop_toBottomOf="@id/addressDivider"
            android:paddingLeft="@dimen/common_padding"
            android:paddingRight="@dimen/common_padding"
            android:gravity="center"
            android:text="hhhhhh"
            android:textSize="@dimen/text_middle_size"
            android:textColor="@color/black"
            tools:ignore="MissingConstraints" />
        <TextView
            android:id="@+id/addressChang"
            android:layout_width="wrap_content"
            android:layout_height="35dp"
            app:layout_constraintRight_toRightOf="parent"
            app:layout_constraintTop_toBottomOf="@id/addressDivider"
            android:gravity="center"
            android:text="修改"
            android:textSize="@dimen/text_middle_size"
            android:textColor="@color/cardview_dark_background"
            android:paddingLeft="@dimen/common_padding"
            android:paddingRight="@dimen/common_padding"/>
    </androidx.constraintlayout.widget.ConstraintLayout>
    <androidx.constraintlayout.widget.ConstraintLayout
        android:id="@+id/addressPartTow"
        android:layout_width="match_parent"
        android:layout_height="@dimen/common_line_height"
        app:layout_constraintTop_toBottomOf="@id/addressPartOne"
        tools:ignore="MissingConstraints">

        <TextView
            android:id="@+id/addressHometown"
            android:layout_width="wrap_content"
            android:layout_height="25dp"
            android:gravity="center"
            android:paddingLeft="@dimen/common_padding"
            android:paddingRight="@dimen/common_padding"
            android:text="hhhhh"
            android:textColor="@color/cardview_dark_background"
            android:textSize="@dimen/text_small_size"
            app:layout_constraintTop_toTopOf="parent" />
        <TextView
            android:id="@+id/addressHome"
            android:layout_width="wrap_content"
            android:layout_height="25dp"
            app:layout_constraintTop_toBottomOf="@id/addressHometown"
            android:text="hhhhh"
            android:textSize="@dimen/text_small_size"
            android:textColor="@color/cardview_dark_background"
            android:paddingLeft="@dimen/common_padding"
            android:paddingRight="@dimen/common_padding"/>
        <TextView
            android:layout_width="wrap_content"
            android:layout_height="match_parent"
            app:layout_constraintRight_toRightOf="parent"
            android:gravity="center"
            android:text="默认"
            android:textSize="@dimen/text_middle_size"
            android:textColor="@color/common_colorPrimary"
            android:paddingLeft="@dimen/common_padding"
            android:paddingRight="@dimen/common_padding"/>
    </androidx.constraintlayout.widget.ConstraintLayout>

</androidx.constraintlayout.widget.ConstraintLayout>