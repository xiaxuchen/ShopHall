<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.cxyz.message">
    <application
        android:name="com.cxyz.context.application.BaseApplication"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/CommonAppTheme">

        <activity android:name=".ui.Activity.GoodsInfoActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".ui.Activity.TestFragment"></activity>
        <activity android:name=".ui.Activity.MainActivity"></activity>
        <activity android:name=".ui.Activity.ChatInfoActivity"></activity>
        <activity android:name=".ui.Activity.ChatMessageActivity"></activity>
        <activity android:name=".ui.Activity.LoginTestActicity"></activity>
        <meta-data
            android:name="com.cxyz.starter.iconify"
            android:value="com.cxyz.message.IconifyStarter" />
    </application>
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE"/>
</manifest>